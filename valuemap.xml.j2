<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>3.0</version>
    <date>{{ ansible_date_time.iso8601 }}</date>
    <value_maps>
        <value_map>
            <name>{{ item.mapname }}</name>
            <mappings>
{% for mapping in item.mappings %}
                <mapping>
                    <value>{{ mapping.value }}</value>
                    <newvalue>{{ mapping.newvalue }}</newvalue>
                </mapping>
{% endfor %}
            </mappings>
        </value_map>
    </value_maps>
</zabbix_export>
